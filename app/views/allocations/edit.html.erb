
<%= simple_form_for @allocation do |f| %>

<%= f.error_notification %>

<%= render 'form', :f => f %>

<%= link_to_add_association fa_icon("plus-circle"), f, :items, class: "btn btn-secondary", "data-association-insertion-node" => "#insertionPoint" %>
<%= f.submit "Salvar", class: "btn btn-primary" %>

<% end %>